{
  "name": "worona-dashboard-client",
  "version": "1.0.0",
  "description": "Worona dashboard client core package",
  "main": "index.js",
  "scripts": {
    "clean:dev:vendors": "rimraf dev/vendors",
    "clean:prod:vendors": "rimraf prod/vendors",
    "clean:dev:core": "rimraf dev/core",
    "clean:prod:core": "rimraf prod/core",
    "build:dev:vendors": "npm run clean:dev:vendors && webpack --config webpack.dev.vendors.js --progress",
    "build:prod:vendors": "npm run clean:prod:vendors && webpack --config webpack.prod.vendors.js --progress -p",
    "build:dev:core": "npm run clean:dev:core && webpack --config webpack.dev.core.js --progress",
    "build:prod:core": "npm run clean:prod:core && webpack --config webpack.prod.core.js --progress -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worona/worona-core.git"
  },
  "keywords": [
    "worona",
    "wordpress",
    "amp",
    "app",
    "mobile",
    "fbia"
  ],
  "author": "Worona Labs SL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/worona/worona-core/issues"
  },
  "homepage": "https://github.com/worona/worona-core#readme",
  "dependencies": {
    "babel-polyfill": "^6.7.4",
    "classnames": "^2.2.3",
    "fastclick": "^1.0.6",
    "i18next": "^3.1.0",
    "i18next-xhr-backend": "^0.5.4",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-i18next": "^1.4.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.1",
    "redux": "^3.3.1",
    "redux-saga": "^0.10.0",
    "reselect": "^2.5.1"
  },
  "devDependencies": {
    "rimraf": "^2.5.2",
    "webpack": "^1.13.1"
  }
}
