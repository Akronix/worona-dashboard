{
  "name": "worona-core",
  "version": "1.0.0",
  "description": "Worona core project",
  "scripts": {
    "start": "npm run client",
    "server": "(cd worona-dashboard-server && meteor)",
    "client": "node webpack-dev-server.js",
    "eslint": "eslint ./core ./dev --ext .js --ext .jsx",
    "install:all": "npm install && npm run install:app-client && npm run install:dashboard-client && npm run install:dashboard-server",
    "install:app-client": "(cd worona-app-client && npm install)",
    "install:dashboard-client": "(cd worona-dashboard-client && npm install)",
    "install:dashboard-server": "(cd worona-dashboard-server && npm install)",
    "link": "npm run link:app-client && npm run link:dashboard-client && npm run link:dashboard-server",
    "link:app-client": "(cd worona-app-client && npm link)",
    "link:dashboard-client": "(cd worona-dashboard-client && npm link)",
    "link:dashboard-server": "(cd worona-dashboard-server && npm link)",
    "build:test": "webpack --config webpack.config.test.js",
    "test": "ava worona*/**/tests/**/*.js --watch --verbose --require babel-register --require ./test-globals.js --require babel-polyfill",
    "test-coverage": "ava worona*/**/tests/**/*.js --verbose --require babel-register --require ./test-globals.js --require babel-polyfill",
    "coverage": "nyc --require babel-polyfill --require babel-register --cache npm run test-coverage",
    "storybook": "start-storybook -p 9001",
    "mongo": "(cd worona-dashboard-server && meteor mongo)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worona/worona-core.git"
  },
  "keywords": [
    "worona",
    "wordpress",
    "amp",
    "app",
    "mobile",
    "fbia"
  ],
  "author": "Worona Labs SL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/worona/worona-core/issues"
  },
  "homepage": "https://github.com/worona/worona-core#readme",
  "devDependencies": {
    "@kadira/storybook": "^1.5.0",
    "ava": "^0.14.0",
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-resolver": "0.0.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.5.3",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "nyc": "^6.4.0",
    "postcss": "^5.0.19",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.8.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.1",
    "tape": "^4.5.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "ws": "^1.1.0"
  },
  "dependencies": {}
}
