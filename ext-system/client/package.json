{
  "name": "worona-dashboard-client",
  "version": "1.0.0",
  "description": "Worona dashboard client",
  "main": "index.js",
  "scripts": {
    "clean:dev": "rimraf dist/dev",
    "clean:prod": "rimraf dist/prod",
    "build:dev": "npm run clean:dev && webpack --config webpack.dev.js --progress",
    "build:prod": "npm run clean:prod && webpack --config webpack.prod.js --progress -p",
    "build:dev:modules": "npm run clean:dev && webpack --config webpack.dev.js --progress --display-modules",
    "build:prod:modules": "npm run clean:prod && webpack --config webpack.prod.js --progress -p --display-modules",
    "build:dev:reasons": "npm run clean:dev && webpack --config webpack.dev.js --progress --display-modules --display-reasons",
    "build:prod:reasons": "npm run clean:prod && webpack --config webpack.prod.js --progress -p --display-modules --display-reasons",
    "build": "npm run build:dev && npm run build:prod",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "test": "ava **/tests/**/*.js --watch --verbose --require babel-register --require babel-polyfill",
    "update:vendors": "npm install --save --save-exact vendors-dashboard-worona@latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worona/worona-core.git"
  },
  "keywords": [
    "worona",
    "wordpress",
    "amp",
    "app",
    "mobile",
    "fbia"
  ],
  "author": "Worona Labs SL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/worona/worona-core/issues"
  },
  "homepage": "https://github.com/worona/worona-core#readme",
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "classnames": "^2.2.5",
    "lodash": "^4.13.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-saga": "^0.10.5",
    "reselect": "^2.5.1",
    "worona": "^1.0.0"
  },
  "devDependencies": {
    "ava": "^0.15.1",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "bundle-loader": "^0.5.4",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.17.0",
    "json-loader": "^0.5.4",
    "mock-require": "^1.3.0",
    "node-sass": "^3.7.0",
    "postcss": "^5.0.21",
    "postcss-cssnext": "^2.5.2",
    "postcss-loader": "^0.9.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
