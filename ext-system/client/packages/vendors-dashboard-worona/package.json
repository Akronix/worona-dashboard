{
  "name": "vendors-dashboard-worona",
  "version": "1.2.4",
  "description": "Package to store the Worona vendors in Webpack's DLL format",
  "main": "",
  "scripts": {
    "clean": "rimraf dist",
    "clean:dev": "rimraf dist/dev",
    "clean:prod": "rimraf dist/prod",
    "build:dev": "npm run clean:dev && webpack --progress --config webpack.dev.js",
    "build:prod": "npm run clean:prod && webpack -p --progress --config webpack.prod.js",
    "build:dev:modules": "npm run clean:dev && webpack --progress --config webpack.dev.js --display-modules",
    "build:prod:modules": "npm run clean:prod && webpack -p --progress --config webpack.prod.js --display-modules",
    "build:dev:reasons": "npm run clean:dev && webpack --progress --config webpack.dev.js --display-modules --display-reasons",
    "build:prod:reasons": "npm run clean:prod && webpack -p --progress --config webpack.prod.js --display-modules --display-reasons",
    "build": "npm run build:dev && npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worona/worona.git"
  },
  "keywords": [
    "worona",
    "dashboard",
    "vendors"
  ],
  "author": "Worona Labs SL | Luis Herranz, @luisherranz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/worona/worona/issues"
  },
  "homepage": "https://github.com/worona/worona#readme",
  "worona": {
    "niceName": "Vendors",
    "slug": "vendors",
    "namespace": "vendor",
    "type": "vendor",
    "service": "dashboard",
    "authors": [
      "luis@worona.org"
    ],
    "default": 1,
    "core": 1,
    "listed": 0,
    "deactivable": 0,
    "public": 1,
